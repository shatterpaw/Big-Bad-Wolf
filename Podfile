  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Big Bad Wolf
  pod 'ReSwift'
  pod 'ReSwiftRouter'

  target 'Big Bad WolfTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Big Bad WolfUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
